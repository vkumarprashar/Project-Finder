<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="BeanClasses.DemandRequest" table="DEMANDPROJECTS">
        <id name="sno" type="int">
            <column name="SNO" not-null="false" default="null"/>
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String" length="60">
            <column name="NAME" />
        </property>
        <property name="email" type="java.lang.String" length="60">
            <column name="EMAIL" />
        </property>
        <property name="title" type="java.lang.String" length="50">
            <column name="TITLE" />
        </property>
        <property name="technology" type="java.lang.String" length="40">
            <column name="TECHNOLOGY" />
        </property>
        <property name="contact" type="java.lang.String" length="15">
            <column name="CONTACT" />
        </property>
        <property name="detail" type="java.lang.String" >
            <column name="DETAIL" />
        </property>
        <property name="status" type="int" length="1">
            <column name="STATUS" />
        </property>
    </class>
</hibernate-mapping>
